  },
  "homepage": "https://github.com/Advanced-Computer-Lab-2022/Hakim#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "husky": "^8.0.0",
    "prettier": "2.7.1"
  },
  "dependencies": {
    "use-react-router-breadcrumbs": "^4.0.1"
  }
}